{
  "name": "codylane-beaker_init",
  "version": "2015.4.0",
  "author": "Cody Lane",
  "summary": "A puppet module for building out a skeleton directory for your beaker tests",
  "license": "Apache 2.0",
  "source": "https://github.com/codylane/puppet-beaker_init.git",
  "project_page": "https://github.com/puppetlabs/beaker.git",
  "issues_url": "https://github.com/codylane/puppet-beaker_init.git",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 1.0.0"}
